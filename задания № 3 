#include <iostream>
#include <cstdlib>
#include <ctime>   
using namespace std;

int main() {
    
    const int lignes = 4;
    const int colonnes = 5;
    int tableau[lignes][colonnes];

   
  
    for (int i = 0; i < lignes; i++) {
        for (int j = 0; j < colonnes; j++) {
            tableau[i][j] = rand() % 100; // nombres aléatoires entre 0 et 99
        }
    }

   
    cout << "Tableau bidimensionnel (" << lignes << "x" << colonnes << ") :" << endl;
    for (int i = 0; i < lignes; i++) {
        for (int j = 0; j < colonnes; j++) {
            cout << tableau[i][j] << "\t"; // \t pour bien aligner les colonnes
        }
        cout << endl;
    }

  
    cout << endl << "Affichage terminé " << endl;

    return 0;
}